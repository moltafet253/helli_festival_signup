{
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "npm-run-all build-app copy-icons copy-fonts",
        "build-app": "vite build",
        "copy-icons": "mkdir -p public/build/assets/icons && cp -r resources/images/icons public/build/assets/",
        "copy-fonts": "mkdir -p public/build/assets && find resources/css/Fonts/ -type f -exec cp {} public/build/assets/ \\;",
        "build:prod": "cross-env NODE_ENV=production vite build"
    },
    "devDependencies": {
        "@popperjs/core": "^2.11.6",
        "@vitejs/plugin-vue": "^4.0.0",
        "autoprefixer": "^10.4.14",
        "axios": "^1.4.0",
        "bootstrap": "^5.2.3",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "laravel-vite-plugin": "^0.7.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.21",
        "sass": "^1.56.1",
        "tailwindcss": "^3.3.1",
        "vite": "^4.0.0",
        "vite-plugin-vue2": "^2.0.3",
        "vue": "^3.2.37",
        "vue-loader": "17.0.1",
        "vue-template-compiler": "2.7.14",
        "webpack": "^5.77.0",
        "webpack-cli": "^5.0.1",
        "webpack-obfuscator": "^3.5.1"
    },
    "dependencies": {
